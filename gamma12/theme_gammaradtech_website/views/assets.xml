<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template id="mystyle" name="My style" inherit_id="website.assets_frontend">
        <xpath expr="link[last()]" position="after">
            <!-- FONTS -->
            <link rel="stylesheet" type="text/css"
                  href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.2/animate.css"/>
            <!-- FONTS END-->
            <link href="https://fonts.googleapis.com/css?family=Poppins:100,300,500" rel="stylesheet"/>
            <!--
            CSS
            ============================================= -->
            <link rel="stylesheet" href="/theme_gammaradtech_website/static/css/linearicons.css"/>
            <link rel="stylesheet" href="/theme_gammaradtech_website/static/css/owl.carousel.css"/>
            <link rel="stylesheet" href="/theme_gammaradtech_website/static/css/font-awesome.min.css"/>
            <link rel="stylesheet" href="/theme_gammaradtech_website/static/css/nice-select.css"/>
            <link rel="stylesheet" href="/theme_gammaradtech_website/static/css/magnific-popup.css"/>
            <link rel="stylesheet" href="/theme_gammaradtech_website/static/css/bootstrap.css"/>
            <link rel="stylesheet" href="/theme_gammaradtech_website/static/scss/main.scss"/>
        </xpath>
    </template>
    <template id="myjs" inherit_id="website.assets_frontend" name="My Js">
        <xpath expr="script[last()]" position="after">
            <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"
                    integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM"
                    crossorigin="anonymous"></script>
            <script src="https://unpkg.com/masonry-layout@4/dist/masonry.pkgd.min.js"></script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js"
                    integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4"
                    crossorigin="anonymous"></script>
            <script src="/theme_gammaradtech_website/static/js/jquery.ajaxchimp.min.js"></script>
            <script src="/theme_gammaradtech_website/static/js/owl.carousel.min.js"></script>
            <script src="/theme_gammaradtech_website/static/js/jquery.nice-select.min.js"></script>
            <script src="/theme_gammaradtech_website/static/js/jquery.magnific-popup.min.js"></script>
            <script src="/theme_gammaradtech_website/static/js/jquery.counterup.min.js"></script>
            <script src="/theme_gammaradtech_website/static/js/waypoints.min.js"></script>
            <script src="/theme_gammaradtech_website/static/js/build.js"></script>
            <!-- JS END -->
        </xpath>
    </template>

    <template id="my_js" inherit_id="website.assets_editor" name="My Js">
        <xpath expr="script[last()]" position="after">
            <script type="text/javascript" src="/theme_gammaradtech_website/static/js/website_editor.js"/>
        </xpath>
    </template>

